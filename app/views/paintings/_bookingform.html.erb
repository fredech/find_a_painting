<div class="form">
  <%= simple_form_for [@painting, booking]  do |f| %>
    <%= f.input :start_date, label: 'Start Date', placeholder: 'dd/mm/yyyy' %>
    <%= f.input :end_date, label: 'End Date', placeholder: 'dd/mm/yyyy' %>
    <%= f.button :submit, "Book!", class: "btn-medium" %>
  <% end %>
</div>
