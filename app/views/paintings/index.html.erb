<div class="container">
  <h4><%= @paintings.length %> Paintings for you</h4>
  <div class="row">
    <% @paintings.each do |painting| %>
      <%= render 'shared/painting_card', painting: painting %>
    <% end %>
  </div>
</div>
<br>
<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
